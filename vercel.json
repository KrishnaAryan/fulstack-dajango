
{
    "version": 2,
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.8",
          "pipenv": true
        }
      },
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "command": "python manage.py collectstatic --noinput"
        }
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "shoping_india.settings"
    }
  }
  